<script>
  function mostrarPopup() {
    document.getElementById('popup-backdrop').style.display = 'block';
    document.getElementById('popup-wrapper').style.display = 'block';
    document.getElementById('popup-ribbon-container').style.display = 'block';
  }

  function cerrarPopup() {
    document.getElementById('popup-backdrop').style.display = 'none';
    document.getElementById('popup-wrapper').style.display = 'none';
    document.getElementById('popup-ribbon-container').style.display = 'none';
  }

  setTimeout(mostrarPopup, 5000);
</script>

